' {$STAMP BS2}
' {$PBASIC 2.5}
' ROBOT ASSEMBLY AND TESTING 2

DEBUG "Program Running"
'Straight
counter VAR Word
FOR counter = 1 TO 122
  PULSOUT 15, 650
  PULSOUT 14, 850
  PAUSE 20
NEXT
'Stand Still
FOR Counter = 1 TO 40
  PULSOUT 15, 750
  PULSOUT 14, 750
  PAUSE 20
NEXT
'BackWords
FOR COUNTER = 1 TO 122
  PULSOUT 15, 850
  PULSOUT 14, 650
  PAUSE 20
NEXT

END